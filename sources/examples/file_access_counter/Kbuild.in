module_name=@module_name@

interceptors_normal = file_system_type super_operations inode_operations dentry_operations
interceptors_foreign = inode_file_operations
interceptors_standard = file_operations

ccflags-y :=  -I@KEDR_COI_INCLUDE_DIR@ -I@KEDR_INSTALL_DIR@/include
obj-m := ${module_name}.o
${module_name}-y := file_access_counter.o functions_support.o
${module_name}-y += $(addsuffix _interceptor.o, $(interceptors_normal) $(interceptors_foreign) $(interceptors_standard))