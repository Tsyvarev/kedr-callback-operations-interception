#include <linux/cdev.h>
#include <linux/fs.h>
<<

object.type = struct cdev
object.operations_field = ops

foreign_object.type = struct file
foreign_object.operations_field = f_op

operations.type = struct file_operations

[group]
operation.name = open
operation.returnType = int

operation.arg.type = struct inode *
operation.arg.name = inode

operation.arg.type = struct file *
operation.arg.name = filp

operation.object = inode->i_cdev
operation.foreign_object = filp

operation.default = return 0;